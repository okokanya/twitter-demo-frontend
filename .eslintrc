{
  "extends": ["react-app", "airbnb"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "arrow-parens": "off",
    "no-confusing-arrow": "off",
    "jsx-a11y/anchor-is-valid": 0,
    "object-curly-newline": 0,
    "class-methods-use-this": 0
    }
}
